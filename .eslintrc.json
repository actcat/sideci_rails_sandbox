{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:flowtype/recommended",
    "airbnb",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "modules": true
    }
  },
  "plugins": [
    "react",
    "flowtype"
  ],
  "parser": "babel-eslint"
}
